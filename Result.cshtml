@model DojoSurvey.Models.Home.Survey;

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <title>Result</title>
</head>
<body>
    <div class="card">
        <div class="card-header">
            <h3>Results</h3>
        </div>
        <div class="card-body">
            <div class="body title">
                <h4 class="text-center">Submitted Info </h4>
                <h4 class="text-center">Name: @Model.StudentName</h4>
                <h4 class="text-center">Dojo Location: @Model.Location</h4>
                <h4 class="text-center">Favorite Language: @Model.FavLanguage</h4>
                <h4 class="text-center">Comment: @Model.Comment</h4>
            </div>
            <a href="http://localhost:5000">
                <button class="btn btn-warning">Go Back</button>
            </a>
        </div>
    </div>
</body>
</html>